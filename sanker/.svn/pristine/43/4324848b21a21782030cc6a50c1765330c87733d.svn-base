<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.sanker.entity.gift">
	<class name="GiftGiving" table="giftGiving">
		<id name="id">
			<column name="id" />
			<generator class="uuid"></generator>
		</id>
		<property name="senderId" type="string"/>
		<property name="receiverId" type="string"/>
		<property name="giftName" type="string"/>
		<property name="giftNum" type="integer"/>
		<property name="givingTime" type="date"/>
	</class>
</hibernate-mapping>
