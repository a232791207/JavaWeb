package com.sanker.entity.player;

import java.io.Serializable;

import com.google.gson.annotations.Expose;

/**
 * 玩家游戏数据
 * @author 滕洁
 * @date 2017-1-19
 */
public class PlayerGameInfo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private Integer id;
	@Expose
	private String playerId;
	@Expose
	private Integer trophy1;//积分榜第一名奖杯数
	@Expose
	private Integer trophy2;
	@Expose
	private Integer trophy3;
	@Expose
	private Integer matchT1;//比赛奖杯数
	@Expose
	private Integer matchT2;
	@Expose
	private Integer matchT3;
	@Expose
	private Integer gameNum;//all Game num
	@Expose
	private Integer winNum;
	@Expose
	private Integer continuityWin;//连胜
	@Expose
	private Integer maxMultiple;
	@Expose
	private	String maxMultipleStr;
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getPlayerId() {
		return playerId;
	}
	public void setPlayerId(String playerId) {
		this.playerId = playerId;
	}
	public Integer getTrophy1() {
		return trophy1;
	}
	public void setTrophy1(Integer trophy1) {
		this.trophy1 = trophy1;
	}
	public Integer getTrophy2() {
		return trophy2;
	}
	public void setTrophy2(Integer trophy2) {
		this.trophy2 = trophy2;
	}
	public Integer getTrophy3() {
		return trophy3;
	}
	public void setTrophy3(Integer trophy3) {
		this.trophy3 = trophy3;
	}
	public Integer getMatchT1() {
		return matchT1;
	}
	public void setMatchT1(Integer matchT1) {
		this.matchT1 = matchT1;
	}
	public Integer getMatchT2() {
		return matchT2;
	}
	public void setMatchT2(Integer matchT2) {
		this.matchT2 = matchT2;
	}
	public Integer getMatchT3() {
		return matchT3;
	}
	public void setMatchT3(Integer matchT3) {
		this.matchT3 = matchT3;
	}
	public Integer getGameNum() {
		return gameNum;
	}
	public void setGameNum(Integer gameNum) {
		this.gameNum = gameNum;
	}
	public Integer getWinNum() {
		return winNum;
	}
	public void setWinNum(Integer winNum) {
		this.winNum = winNum;
	}
	public Integer getContinuityWin() {
		return continuityWin;
	}
	public void setContinuityWin(Integer continuityWin) {
		this.continuityWin = continuityWin;
	}
	public Integer getMaxMultiple() {
		return maxMultiple;
	}
	public void setMaxMultiple(Integer maxMultiple) {
		this.maxMultiple = maxMultiple;
	}
	public String getMaxMultipleStr() {
		return maxMultipleStr;
	}
	public void setMaxMultipleStr(String maxMultipleStr) {
		this.maxMultipleStr = maxMultipleStr;
	}
}
